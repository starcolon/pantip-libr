#!/bin/bash

# Prepare report CSV
CSV_PATH="$PANTIPLIBR/data/report.csv"

if [ ! -f "$CSV_PATH" ] 
then
	touch "$CSV_PATH"
else
	rm "$CSV_PATH"
	touch "$CSV_PATH"
fi
echo "DECOM , DIM ,   K , % TOT ,  [0]  ,  [1]  ,  [1]" > "$CSV_PATH"

# Tokenise input
python3 core/process.py

rm data/cluster/*

rm data/hasher/*

# Batch trainings
python3 core/textprocess.py --decom SVD --dim 512 --tagdim 16
python3 core/requeue.py
python3 core/textprocess.py --decom LDA --dim 512 --tagdim 16
# python3 core/requeue.py
# python3 core/textprocess.py --tagdim 16

